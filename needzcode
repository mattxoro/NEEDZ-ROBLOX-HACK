local first = game.Players
local you = first.LocalPlayer.Character.HumanoidRootPart
for i,v in pairs(first:GetPlayers()) do
	for i=1,3 do
you.CFrame = v.Character.HumanoidRootPart.CFrame
wait(.1)
you.CFrame = v.Character.HumanoidRootPart.CFrame		
end
wait(.1)
end
